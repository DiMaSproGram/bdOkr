<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Controller">
   <children>
      <TableView fx:id="table" layoutX="35.0" layoutY="48.0" prefHeight="225.0" prefWidth="551.0">
        <columns>
          <TableColumn fx:id="regionCol" prefWidth="176.0" text="Region" />
          <TableColumn fx:id="redionCenterCol" prefWidth="172.0" text="Region Center" />
            <TableColumn fx:id="areaCol" prefWidth="104.0" text="Area" />
            <TableColumn fx:id="populationCol" prefWidth="97.0" text="Population" />
        </columns>
      </TableView>
      <Button fx:id="deleteBtn" layoutX="424.0" layoutY="327.0" mnemonicParsing="false" onAction="#click" text="Delete" />
      <Button fx:id="addBtn" layoutX="114.0" layoutY="327.0" mnemonicParsing="false" onAction="#clickAdd" text="Add" />
      <MenuBar layoutX="20.0" layoutY="14.0" prefHeight="25.0" prefWidth="361.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
            <Menu fx:id="taskA" mnemonicParsing="false" onAction="#clickA" text="a)">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="b)">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="c)">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
